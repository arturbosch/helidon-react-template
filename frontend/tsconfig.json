{
    "compilerOptions": {
        "composite": true,
        "target": "ES2020",
        "lib": [
            "dom",
            "dom.iterable",
            "ES2020"
        ],
        "rootDir": "./",
        "outDir": "./out",
        "sourceMap": true,
        "allowJs": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "resolveJsonModule": true,
